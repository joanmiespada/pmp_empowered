{

    "parser": "babel-eslint",
    "plugins": ["babel", "react"],
    "env":{
        "browser": true,
        "es6":true,
        "node":true
    },
    "extends": "airbnb", 
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    },
    "rules": {
        "max-len": [1, 70, 2]
        
      }
    
    
}